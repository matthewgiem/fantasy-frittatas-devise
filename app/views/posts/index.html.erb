<h2>home page for post</h2>

<ul id="posts_list">
  <% @posts.each do |post| %>
    <%= render "post", :post => post %>
  <% end %>
</ul>

<hr>

<%= link_to "New Post", new_post_path, id: "new_post_link", remote: true%><br>
<button type="button" name="button" id="cancel_add_post" data-remote="true" >cancel</button>
